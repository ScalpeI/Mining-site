@model IEnumerable<Domain.Entities.Payout>

@{
    ViewBag.Title = @ViewBag.Login + " history payout - Mining operator";
    //Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = "~/Views/Shared/_main.cshtml";
    if (User.Identity.Name != "Admin") Response.Redirect("~/Rates/Index");
}
<div class="panel panel-default">
    <div class="panel-body">
        <table class="table table-striped table-condensed table-bordered">
            <tr>
                <th class="text-center">
                    @Html.DisplayNameFor(model => model.owner)
                </th>
                <th class="text-center">
                    @Html.DisplayNameFor(model => model.count)
                </th>
                <th class="text-center">
                    @Html.DisplayNameFor(model => model.date)
                </th>
                <th class="text-center">
                    Действия
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td class="text-center">
                        @Html.DisplayFor(modelItem => item.owner)
                    </td>
                    <td class="text-center">
                        @Html.DisplayFor(modelItem => item.count)
                    </td>
                    <td class="text-center">
                        @Html.DisplayFor(modelItem => item.date)
                    </td>
                    <td class="text-center">
                        @Html.ActionLink("Edit", "Edit", new { id = item.id, Login = @ViewBag.Login }, new { @class = "btn btn-default" }) |
                        @Html.ActionLink("Details", "Details", new { id = item.id }, new { @class = "btn btn-default" }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.id }, new { @class = "btn btn-danger" })
                    </td>
                </tr>

            }

        </table>
    </div>
    <div class="panel-footer">
        @Html.ActionLink("Добавить", "Create", new { owner = ViewBag.Login }, new { @class = "btn btn-primary" })
        @Html.ActionLink("Вернуться в Админпанель", "Index", "Admin", null, new { @class = "btn btn-default" })
    </div>
</div>